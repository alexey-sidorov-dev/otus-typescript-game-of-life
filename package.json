{
  "name": "otus-typescript-game-of-life",
  "version": "1.0.0",
  "description": "OTUS TypeScript Game of Life",
  "keywords": [
    "otus",
    "typescript"
  ],
  "homepage": "https://github.com/alexey-sidorov-dev/otus-typescript-game-of-lifeg#readme",
  "bugs": {
    "url": "https://github.com/alexey-sidorov-dev/otus-typescript-game-of-life/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-sidorov-dev/otus-typescript-game-of-life.git"
  },
  "license": "ISC",
  "author": "Alexey Sidorov",
  "main": "./src/index.ts",
  "scripts": {
    "build": "webpack build --node-env production",
    "build:analyze": "webpack build --node-env production && webpack --analyze",
    "build:dev": "webpack build --node-env development",
    "build:start": "webpack serve --node-env development --open",
    "lint": "eslint . --ext .js,.ts && tsc --project tsconfig.json --noEmit",
    "lint:all": "eslint . --ext .js,.ts && tsc --project tsconfig.json --noEmit && stylelint **/*.{css,scss} && prettier . --check",
    "lint:all:fix": "eslint . --ext .js,.ts --fix && tsc --project tsconfig.json --noEmit && stylelint **/*.{css,scss} && prettier . --write",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "stylelint": "stylelint **/*.{css,scss}",
    "stylelint:fix": "stylelint **/*.{css,scss} --fix",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@types/eslint": "^8.4.6",
    "@types/jest": "^29.1.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.8.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.25.5",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "prettier-plugin-sh": "^0.12.8",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
